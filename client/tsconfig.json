{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@reducers/*": ["src/store/reducers/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "custom.d.ts"]
}
